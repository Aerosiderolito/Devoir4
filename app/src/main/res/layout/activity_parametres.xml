<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2D2F31"
    android:backgroundTint="#1A21F3FF">

    <Button
        android:id="@+id/button_deconnexion"
        android:layout_width="408dp"
        android:layout_height="90dp"
        android:fontFamily="@font/bangers"
        android:text="Deconnexion"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="531dp" />

    <Button
        android:id="@+id/button_support"
        android:layout_width="408dp"
        android:layout_height="90dp"
        android:fontFamily="@font/bangers"
        android:text="Support"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="424dp" />

    <Button
        android:id="@+id/button_dons"
        android:layout_width="408dp"
        android:layout_height="90dp"
        android:fontFamily="@font/bangers"
        android:text="Dons"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="320dp" />

    <Button
        android:id="@+id/button_prpos"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="408dp"
        android:layout_height="90dp"
        android:backgroundTint="#E8FCFD"
        android:fontFamily="@font/bangers"
        android:text="A propos"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="219dp" />

    <Button
        android:id="@+id/b_ic_messages"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="148dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="Message"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_ic_param"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:textColor="#FFFFFF" />

    <Button
        android:id="@+id/b_ic_param"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="149dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="paramètres"
        android:textColor="#121111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:fontFamily="@font/fredoka_one" />

    <Button
        android:id="@+id/b_ic_contacts"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="127dp"
        android:layout_height="77dp"
        android:layout_marginStart="148dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="Contacts"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_ic_messages"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="413dp"
        android:layout_height="72dp"
        android:background="#009688"
        android:fontFamily="@font/bangers"
        android:text="paramètres"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_access"
        android:layout_width="408dp"
        android:layout_height="90dp"
        android:backgroundTint="#1AEFF2F3"
        android:fontFamily="@font/bangers"
        android:text="Accessibilité"
        android:textAlignment="textStart"
        android:textColor="#0E0E0E"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="118dp" />

</androidx.constraintlayout.widget.ConstraintLayout>