<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2D2F31"
    android:backgroundTint="#1A21F3FF">

    <Button
        android:id="@+id/button_deconnexion"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:backgroundTint="#C0FCFF"
        android:fontFamily="@font/bangers"
        android:text="Deconnexion"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:icon="@drawable/ic_logout"
        app:iconGravity="end"
        app:iconPadding="8dp"
        app:iconSize="50dp"
        app:iconTint="#050505"
        app:layout_constraintBottom_toTopOf="@+id/b_ic_messages"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_support" />

    <Button
        android:id="@+id/button_support"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:backgroundTint="#C0FCFF"
        android:fontFamily="@font/bangers"
        android:text="Support"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:icon="@drawable/ic__agent_24"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#070707"
        app:layout_constraintBottom_toTopOf="@+id/b_ic_contacts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_dons"
        app:layout_constraintVertical_bias="0.049" />

    <Button
        android:id="@+id/button_dons"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:backgroundTint="#C0FCFF"
        android:fontFamily="@font/bangers"
        android:text="Dons"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:icon="@drawable/ic_donate"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#050505"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_securit" />

    <Button
        android:id="@+id/button_securit"
        style="@style/Widget.MaterialComponents.Button.UnelevatedButton"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:backgroundTint="#C0FCFF"
        android:fontFamily="@font/bangers"
        android:text="Sécurité"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:icon="@drawable/ic_security"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#0B0A0A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_access" />

    <Button
        android:id="@+id/b_ic_messages"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="148dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="Boîte de Réception"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_ic_param"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:textColor="#FFFFFF" />

    <Button
        android:id="@+id/b_ic_param"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="149dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="paramètres"
        android:textColor="#121111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:fontFamily="@font/fredoka_one" />

    <Button
        android:id="@+id/b_ic_contacts"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="127dp"
        android:layout_height="77dp"
        android:layout_marginStart="148dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="Contacts"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_ic_messages"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="413dp"
        android:layout_height="72dp"
        android:background="#009688"
        android:fontFamily="@font/bangers"
        android:text="paramètres"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_access"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:backgroundTint="#C0FCFF"
        android:fontFamily="@font/bangers"
        android:text="Accessibilité"
        android:textAlignment="textStart"
        android:textColor="#0E0E0E"
        android:textSize="24sp"
        app:icon="@drawable/ic_handicap"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#101010"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        tools:layout_editor_absoluteY="536dp" />

</androidx.constraintlayout.widget.ConstraintLayout>