<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1A21F3FF">

    <Button
        android:id="@+id/button5"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="409dp"
        android:layout_height="126dp"
        android:layout_marginTop="72dp"
        android:backgroundTint="#00FFFFFF"
        app:icon="@drawable/ic_baseline_mark_chat_read_24"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#101010"
        app:layout_constraintBottom_toTopOf="@+id/b_ic_contacts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.958" />

    <Button
        android:id="@+id/button3"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="409dp"
        android:layout_height="126dp"
        android:layout_marginTop="72dp"
        android:backgroundTint="#00FFFFFF"
        app:icon="@drawable/ic_baseline_mark_chat_read_24"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#101010"
        app:layout_constraintBottom_toTopOf="@+id/b_ic_contacts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.53" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="192dp"
        android:layout_height="67dp"
        android:fontFamily="@font/nixie_one"
        android:text="Hey, how are you? Contact me when you can please.."
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/profPic10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="192dp"
        android:layout_height="67dp"
        android:fontFamily="@font/nixie_one"
        android:text="Whatsupp?"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintHorizontal_bias="0.213"
        app:layout_constraintStart_toStartOf="@+id/textView26"
        app:layout_constraintTop_toTopOf="@+id/button3"
        app:layout_constraintVertical_bias="0.881" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="192dp"
        android:layout_height="67dp"
        android:fontFamily="@font/nixie_one"
        android:text="Are you coming tonight?"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="@+id/button5"
        app:layout_constraintHorizontal_bias="0.566"
        app:layout_constraintStart_toStartOf="@+id/button5"
        app:layout_constraintTop_toTopOf="@+id/button5"
        app:layout_constraintVertical_bias="0.898" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bangers"
        android:text="SAM"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/textView25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/profPic10"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/b_add"
        android:layout_width="82dp"
        android:layout_height="68dp"
        android:autoSizeTextType="none"
        android:backgroundTint="#009688"
        android:text="+"
        android:textAlignment="viewEnd"
        android:textSize="24sp"
        android:tint="#FBFBFB"
        app:icon="@drawable/ic_message"
        app:iconGravity="start|end|textEnd"
        app:iconSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bangers"
        android:text="Dom"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/profPic11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.264"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button3"
        app:layout_constraintVertical_bias="0.194" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bangers"
        android:text="Nick"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/profPic13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.264"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profPic13"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/profPic10"
        android:layout_width="98dp"
        android:layout_height="93dp"
        android:layout_marginTop="132dp"
        app:layout_constraintBottom_toTopOf="@+id/profPic5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.022"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.285"
        app:srcCompat="@drawable/ic_account_box"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/profPic11"
        android:layout_width="98dp"
        android:layout_height="93dp"
        app:layout_constraintBottom_toBottomOf="@+id/button3"
        app:layout_constraintEnd_toStartOf="@+id/textView27"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.914"
        app:srcCompat="@drawable/ic_account_box"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/profPic13"
        android:layout_width="98dp"
        android:layout_height="93dp"
        app:layout_constraintBottom_toBottomOf="@+id/button5"
        app:layout_constraintEnd_toStartOf="@+id/textView29"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button5"
        app:layout_constraintVertical_bias="0.424"
        app:srcCompat="@drawable/ic_account_box"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/b_ic_messages"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="148dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="Boîte de Réception"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_ic_param"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:textColor="@color/black" />

    <Button
        android:id="@+id/b_ic_param"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="149dp"
        android:layout_height="77dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="paramètres"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:fontFamily="@font/fredoka_one" />

    <Button
        android:id="@+id/b_ic_contacts"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="127dp"
        android:layout_height="77dp"
        android:layout_marginStart="148dp"
        android:background="#FFFFFF"
        android:backgroundTint="#009688"
        android:fontFamily="@font/fredoka_one"
        android:text="Contacts"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b_ic_messages"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="413dp"
        android:layout_height="72dp"
        android:background="#009688"
        android:fontFamily="@font/bangers"
        android:text="Boîte de Réception"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
        android:layout_width="409dp"
        android:layout_height="126dp"
        android:layout_marginTop="72dp"
        android:backgroundTint="#00FFFFFF"
        app:icon="@drawable/ic_baseline_mark_chat_unread_24"
        app:iconGravity="end"
        app:iconSize="50dp"
        app:iconTint="#101010"
        app:layout_constraintBottom_toTopOf="@+id/b_ic_contacts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.087" />

</androidx.constraintlayout.widget.ConstraintLayout>